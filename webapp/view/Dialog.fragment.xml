<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog id="Dialog" resizable="false" contentWidth="auto" draggable="true" >
		
		<Input id="FET" placeholder="Search by Barcode" liveChange="onEdes" />
		<Input id="EDES" editable="false"/>
		<Label text="Quantity"/>
		<Input id="FETV" type="Number" change="onDchk"/>
		

		
		<beginButton>
			<Button id="F" text="Fetch" press="onOkDialog"/>
		</beginButton>
		<endButton>
			<Button text="Cancel" press="onCloseDialog"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>